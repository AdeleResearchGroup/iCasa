<ss>

  <!-- Default Component style -->
  <s n="" t="nextapp.echo.app.Component">
    <p n="font" t="Font">
      <f sz="11pt">
        <tf n="Sans-Serif"/>
      </f>
    </p>
  </s>
  
  <!-- Default ContentPane style -->
  <s n="" t="nextapp.echo.app.ContentPane">
    <p n="background" t="Color">#f2f1f0</p>
  </s>
  
  <!-- HousePane ContentPane style -->
  <s n="housePane" t="nextapp.echo.app.ContentPane">
    <p n="background" t="Color">#ffffff</p>
  </s>

  <!-- Default SplitPane style -->
  <s n="" t="nextapp.echo.app.SplitPane">
    <p n="resizable" t="b" v="true" />
    <p n="separatorWidth" t="Extent" v="5px"/>
    <p n="separatorHeight" t="Extent" v="5px"/>
    <p n="separatorHorizontalImage" t="FillImage">
      <fi t="r" r="y">
        <i r="/SeparatorHorizontal.png"/>
      </fi>
    </p>
    <p n="separatorVerticalImage" t="FillImage">
      <fi t="r" r="x">
        <i r="/SeparatorVertical.png"/>
      </fi>
    </p>
  </s>
  
  <!-- Default AccordionPane style -->
  <s n="" t="nextapp.echo.extras.app.AccordionPane">
    <p n="tabBackground" t="Color">#cccccc</p>
    <p n="tabFont" t="Font">
      <f bo="1" />
    </p>
    <p n="tabRolloverEnabled" t="b" v="true"/>
    <p n="tabRolloverBackground" t="Color">#ada9a5</p>
  </s>

  <!-- Default WindowPane style -->
  <s n="" t="nextapp.echo.app.WindowPane">
    <p n="titleFont" t="Font">
      <f sz="12pt" bo="1" />
    </p>
    <p n="titleForeground" t="Color">#ffffff</p>
    <p n="titleBackground" t="Color">#000000</p>
    <p n="iconInsets" t="Insets" v="2px 2px" />
  </s>
  
  <!-- ErrorDialog WindowPane style -->
  <s n="errorDialog" t="nextapp.echo.app.WindowPane">
    <p n="titleBackground" t="Color">#5f1f1f</p>
    <p n="titleForeground" t="Color">#ffffff</p>
  </s>

  <!-- Default Button style -->
  <s n="" t="nextapp.echo.app.Button">
    <p n="font" t="Font">
      <f sz="12pt" />
    </p>
    <p n="textAlignment" t="Alignment">
      <a h="center" v="center"/>
    </p>
    <p n="insets" t="Insets" v="2px 8px" />
    <p n="background" t="Color">#cccccc</p>
    <p n="border" t="Border" v="2px outset #000000" />
    <p n="pressedEnabled" t="b" v="true" />
    <p n="pressedBackground" t="Color">#ada9a5</p>
    <p n="pressedBorder" t="Border" v="2px inset #000000" />
    <p n="disabledBackground" t="Color">#cccccc</p>
    <p n="disabledForeground" t="Color">#ada9a5</p>
  </s>
  
  <!-- Default TextField style -->
  <s n="" t="nextapp.echo.app.TextField">
    <p n="alignment" t="Alignment">
      <a h="center" v="center"/>
    </p>
  </s>

</ss>
